<Window x:Class="IT3B_Kostky.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IT3B_Kostky"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250*"/>
            <ColumnDefinition Width="250*"/>
            <ColumnDefinition Width="250*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>


            <RowDefinition Height="200*"/>
            <RowDefinition Height="200*"/>


        </Grid.RowDefinitions>
        <Label Content="Label" Margin="36,28,36,28" Grid.Column="0" Grid.Row="0"/>
        <Label Content="Label" Margin="36,28,36,28" Grid.Column="1" Grid.Row="0"/>
        <Label Content="Label" Margin="36,28,36,28" Grid.Column="2" Grid.Row="0"/>
        <Label Content="Label" Margin="36,28,36,28" Grid.Column="0" Grid.Row="1"/>
        <Label Content="Label" Margin="36,28,36,28" Grid.Column="1" Grid.Row="1"/>
        <Label Content="Label" Margin="36,28,36,28" Grid.Column="2" Grid.Row="1"/>

    </Grid>
</Window>
